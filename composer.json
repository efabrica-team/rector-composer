{
    "name": "efabrica/rector-composer",
    "type": "rector-extension",
    "license": "MIT",
    "description": "Allows Rector to make changes in composer.json",
    "require": {
        "php": ">= 8.1",
        "ext-json": "*",
        "rector/rector": "^0.14.5",
        "symplify/composer-json-manipulator": "^11.1",
        "composer/semver": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "RectorComposer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RectorComposer\\Tests\\": "tests"
        }
    },
    "extra": {
        "rector": {
            "includes": [
                "config/config.php"
            ]
        }
    }
}
